{
  "name": "release-notes",
  "version": "0.0.0",
  "description": "A webcomponent which displays release notes",
  "main": "dist/release-notes.js",
  "module": "dist/release-notes.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "format": "prettier src/* --write",
    "serve": "web-dev-server --app-index dev/index.html --node-resolve --watch"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript"
  ],
  "author": "litetex",
  "license": "MIT",
  "dependencies": {
    "lit-element": "^2.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@web/dev-server": "^0.1.16",
    "eslint": "^7.25.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  }
}
